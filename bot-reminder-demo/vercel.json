{
    "version": 2,
    "env": {
      "TELEGRAM_TOKEN": "@tungdd_bot_telegram_token",
      "REDIS_URI": "@tungdd_bot_redis_uri"
    },
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
      {
        "src": "/",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/",
        "dest": "/index.js",
        "methods": ["POST"]
      }
    ]
  }